{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "henriksoderlund-website-v2",
	"main": "./src/worker/index.ts",
	"compatibility_date": "2025-08-03",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"observability": {
		"enabled": true
	},
	"upload_source_maps": true,
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist/client",
		"run_worker_first": ["/"]
	}
}